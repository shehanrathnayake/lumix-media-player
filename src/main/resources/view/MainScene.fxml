<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="499.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep11.MainSceneController">
   <children>
      <AnchorPane fx:id="panelRoot" layoutY="375.0" prefHeight="124.0" prefWidth="650.0" style="-fx-background-color: black;">
         <children>
            <Button fx:id="btnAdd" layoutX="5.0" layoutY="4.0" mnemonicParsing="false" onAction="#btnAddOnAction" style="-fx-background-color: #090B06;" text="+ Add" textFill="WHITE">
               <font>
                  <Font size="10.0" />
               </font></Button>
            <Button fx:id="btnPlaylist" layoutX="59.0" layoutY="4.0" mnemonicParsing="false" onAction="#btnPlaylistOnAction" prefHeight="6.0" prefWidth="65.0" style="-fx-background-color: #090B06;" text="Playlist" textFill="WHITE">
               <font>
                  <Font size="10.0" />
               </font></Button>
            <Button fx:id="btnPlay" layoutX="302.0" layoutY="92.0" mnemonicParsing="false" onAction="#btnPlayOnAction" text="Play" />
            <Label fx:id="lblDisplay" alignment="CENTER" layoutY="32.0" prefHeight="32.0" prefWidth="655.0" style="-fx-background-color: #081107;" text="_" textFill="WHITE" />
            <Button fx:id="btnBack" layoutX="223.0" layoutY="92.0" mnemonicParsing="false" onAction="#btnBackOnAction" text="back" />
            <Button fx:id="btnForward" layoutX="374.0" layoutY="92.0" mnemonicParsing="false" onAction="#btnForwardOnAction" text="Forwd" />
            <ProgressBar fx:id="pbTimeline" layoutX="6.0" layoutY="71.0" prefHeight="6.0" prefWidth="644.0" progress="0.0" />
            <Label fx:id="lblEndTime" alignment="CENTER_RIGHT" layoutX="590.0" layoutY="78.0" text="00:00:00" />
            <Label fx:id="lblStartTime" layoutX="8.0" layoutY="78.0" text="00:00:00" />
            <Button fx:id="btnStop" layoutX="449.0" layoutY="92.0" mnemonicParsing="false" onAction="#btnStopOnAction" text="Stop" />
         </children>
      </AnchorPane>
      <HBox fx:id="browseRoot" alignment="CENTER_LEFT" layoutY="336.0" prefHeight="39.0" prefWidth="634.0" spacing="5.0" style="-fx-background-color: black;">
         <children>
            <TextField fx:id="txtBrowse" editable="false" prefHeight="24.0" prefWidth="491.0">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </TextField>
            <Button fx:id="btnBrowse" mnemonicParsing="false" onAction="#btnBrowseOnAction" text="Browse" />
            <Button fx:id="btnCloseBrowse" mnemonicParsing="false" onAction="#btnCloseBrowseOnAction" text="Close" />
         </children>
      </HBox>
   </children>
</AnchorPane>
